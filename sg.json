{
   "Outputs": 
   { 	
			"WebServerSG": { 
						"Description": "The security groupID allows Http & Https", 
						"Value": { "Fn::GetAtt": ["DemoSg", "GroupId"] }, 
						"Export": {	   "Name": 
											{ 
												"Fn::Sub": "${AWS::StackName}-SecurityGroupID" 
											} 
								  }  
								} 
   }, 

	"Resources": {
		"DemoSg": {
			"Type": "AWS::EC2::SecurityGroup",
			
			"Properties": {
			"GroupDescription" : "security Group",
			"Tags":[ {
			"Key": "Owner","Value": "Apoorv"
			}],
				"SecurityGroupIngress": [{
						"IpProtocol": "tcp",
						"FromPort": "22",
						"ToPort": "22",
						"CidrIp": "0.0.0.0/0"
					},
					{
						"IpProtocol": "tcp",
						"FromPort": "80",
						"ToPort": "80",
						"CidrIp": "0.0.0.0/0"
					},
					{
						"IpProtocol": "tcp",
						"FromPort": "443",
						"ToPort": "443",
						"CidrIp": "0.0.0.0/0"
					}
				]
			}
		}
	}
}